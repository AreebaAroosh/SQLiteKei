<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:SQLiteKei.Resources.Themes.Dark">
    <!-- Colors -->
    <!---->
    
         
    <Style TargetType="{x:Type MenuItem}">
        <Setter Property="Background" Value="{DynamicResource ApplicationBackground}" />
        <Setter Property="Foreground" Value="{DynamicResource ApplicationFontColor}" />
    </Style>

    <Style x:Key="{x:Static MenuItem.SeparatorStyleKey}" TargetType="{x:Type Separator}">
        <Setter Property="Foreground" Value="{DynamicResource ApplicationBackground}" />
        
    </Style>

</ResourceDictionary>