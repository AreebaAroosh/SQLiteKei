             xmlns:local="clr-namespace:SQLiteKei"
             xmlns:views="clr-namespace:SQLiteKei.Views"
             StartupUri="Views/MainWindow.xaml"
             DispatcherUnhandledException="App_DispatcherUnhandledException">
